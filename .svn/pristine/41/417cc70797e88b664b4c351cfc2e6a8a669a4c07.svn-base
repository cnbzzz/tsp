public class HeatMapDto extends BaseDto {

    private Integer min;

    private Integer max;

    private List<NameValDto> dataList;

    public Integer getMin() {
        return min;
    }

    public void setMin(Integer min) {
        this.min = min;
    }

    public Integer getMax() {
        return max;
    }

    public void setMax(Integer max) {
